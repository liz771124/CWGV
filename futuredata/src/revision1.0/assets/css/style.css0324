.recommendCarousel .info p{display: none;}

.recommendCarousel{ overflow: hidden; }
.recommendCarousel { width: calc(100% + 30px); margin: 0 0 0 -15px; }
.recommendCarousel .slick-list{padding: 0 !important;}
.recommendCarousel .slick-prev:hover, .recommendCarousel .slick-prev:focus, .recommendCarousel .slick-next:hover, .recommendCarousel .slick-next:focus { color: transparent; outline: none; background: transparent; }
.recommendCarousel .slick-prev, .recommendCarousel .slick-next { font-size: 0; line-height: 0; position: absolute; top: auto !important; bottom: 0 !important; display: block; width: 23px; height: 23px; padding: 0; -webkit-transform: translate(0, -50%); -ms-transform: translate(0, -50%); transform: translate(0, -50%); cursor: pointer; color: transparent; border: none; outline: none; background: none; }
.recommendCarousel .slick-prev { left: 50%; margin-left: -23px; }
.recommendCarousel .slick-next { right: 50%; margin-right: -23px;}
.recommendCarousel .slick-prev:before, .recommendCarousel .slick-next:before {position: absolute; display: block; content: ""; top: 50%;width: 21px;height: 18px;transform: translate(-50%, -50%);}
.recommendCarousel .slick-prev:before { left: 50%; background: url(../img/arrow-prev.png)no-repeat center center;background-size: contain;}
.recommendCarousel .slick-next:before { right: 50%;background: url(../img/arrow-next.png)no-repeat center center;background-size: contain;}
.recommendCarousel .slick-list>.slick-track { display: -webkit-flex; display: -ms-flexbox; display: flex !important; }
.recommendCarousel .slick-slide { margin: 0 30px;padding-top: 100px;height: auto; background: #fff;transition: all .3s ease-in-out;}
.recommendCarousel .item a{ display: block; }
.recommendCarousel .infoBox{transition: all .2s ease-in-out;}
.recommendCarousel .imgBox { position: relative;}
.recommendCarousel .imgBox .imgInnerBox{border: 5px solid transparent;}
.recommendCarousel .imgBox img { height: 200px; }
.recommendCarousel .imgBox .videoIcon{ position: absolute; right: 5px; bottom: 5px;width: 45px;height: 45px;line-height: 45px; color: #fff; text-align: center; font-size: 1.3em; }
.child .recommendCarousel .imgBox .videoIcon{background: #00a7df;}
.young .recommendCarousel .imgBox .videoIcon{ background: #8bc520; }
.recommendCarousel .info{ padding: 15px 0; position: relative; }
.recommendCarousel .info h3{font-size: 1.125em;color: #000; line-height: 1.4; font-weight: 500;padding: 0;margin: 0; }
.recommendCarousel .slick-dots{ position: static; width: 100%; padding: 0; margin: 20px 0 10px -10px; list-style: none; text-align: center; }
.recommendCarousel .slick-dots li{ position: relative; display: inline-block; width: 8px; height: 8px; margin: 5px 5px; padding: 0; background: rgba(0,0,0, .5); cursor: pointer; -webkit-border-radius: 50%; -moz-border-radius: 50%; border-radius: 50%; float: none; }
.recommendCarousel .slick-dots li.slick-active { background: rgba(0,0,0,1); }
.recommendCarousel .slick-dots li.slick-active:before{content: "";position: absolute;width: 14px;height: 14px;background: transparent;border: 1px solid #00A7DF;border-radius: 50%;left: -3px;top: -3px;}
.recommendCarousel .slick-dots li button{ position: absolute; left: 50%; width: 100%; margin-left: -6px; background: transparent; cursor: pointer; color: transparent; border: 0; outline: none; }
.recommendCarousel .slick-track .slick-slide.slick-current + .slick-active {padding-top: 0;}
.recommendCarousel .slick-track .slick-slide.slick-current + .slick-active .infoBox{transform: scale(1.1);}
.recommendCarousel .slick-track .slick-slide.slick-current + .slick-active .imgBox{box-shadow: 0 0 10px rgb(0 0 0 / 20%);}
.recommendCarousel .slick-track .slick-slide.slick-current + .slick-active .imgBox .imgInnerBox{border-color: #fff;}
.recommendCarousel .slick-track .slick-slide.slick-current + .slick-active .imgBox:before{content: "";width: 100%;height: 150px;background: url(../img/shadow.png)no-repeat center bottom;background-size: contain;position: absolute;bottom: -40px;left: 0;opacity: .7;z-index: -1;}

