.rightBanner.type02{color: #fff;padding: 20px;display: flex;flex-direction: column;justify-content: space-between;}
.young .rightBanner.type02{background: #8bc520;}
.child .rightBanner.type02{background: #00A7DF;}
.rightBanner.type02 .actionGP{display: flex;flex-direction: column;}
.rightBanner.type02 a h2{color: #fff;text-align: center;}
.rightBanner.type02 a h2:before{position: absolute;height: 1px;top: 7px;content: "";display: block;width: 40px;background: #fff;z-index: 0;transform: rotate(-60deg);right: -5px;}
.rightBanner.type02 a h2:after{width: 40px;background: #fff;z-index: 0;transform: rotate(60deg);left: -5px;}
.rightBanner.type02 a h2 span{padding: 0;}
.rightBanner.type02 a h2 span:after{display: none;}
.rightBanner.type02 .imgBox{box-shadow:0 0 10px rgba(0,0,0,.2);}
.rightBanner.type02 .imgBox img{height: 275px;}
.rightBanner.type02 .freeListen{height: auto;background: #fff;padding: 12px;border-radius: 25px;position: relative;margin-top: 15px;}
.young .rightBanner.type02 .freeListen{color: #8bc520 !important;}
.child .rightBanner.type02 .freeListen{color: #00A7DF !important;}
.child .rightBanner.type02 .imgBox .freeListen{position: absolute;bottom: -25px;left: 50%;transform: translateX(-50%);width: 100px;height: 100px;border-radius: 50%;box-shadow: 0 0 10px rgb(0 0 0 / 30%);}
.rightBanner.type02 .freeListen span{position: absolute;left: 20px;font-size: 1.25em;}
.rightBanner.type02 .freeListen span:before,.rightBanner.type02 .freeListen span:after{display: none;}
.child .rightBanner.type02 .imgBox .freeListen span{left: 50%;transform: translateX(-50%);top: 10px;}
.child .rightBanner.type02 .imgBox .freeListen em{padding-top: 10px;margin: 0;}

