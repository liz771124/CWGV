.freeReadImg .readMore{width: 170px;height: 170px;background: #fff;border: 5px solid ;border-radius: 50%;display: flex;justify-content: center;align-items: center;flex-direction: column;position: absolute;top: 50%;left: 50%;transform: translate(-50%,-50%);transition: all .20s ease-in-out;}
